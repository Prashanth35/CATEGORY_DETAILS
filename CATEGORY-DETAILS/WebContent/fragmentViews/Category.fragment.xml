<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ActionSheet
		title="Choose Your Action"
		showCancelButton="true"
		placement="Bottom">
			<Button text="{category/vegetables}" icon="sap-icon://accept" press="actionSelected" />
			<Button text="{category/non-vegetables}" icon="sap-icon://decline" press="actionSelected" />
			
	</ActionSheet>
</core:FragmentDefinition>